{
    "name": "core",
    "version": "0.1.0",
    "contributions": {
        "types": {
            "error-code": {
                "type": "enum",
                "enumerators": {
                    "parse-error": -32700,
                    "invalid-request": -32600,
                    "method-not-found": -32601,
                    "invalid-params": -32602,
                    "internal-error": -32603,
                    "request-cancelled": -32000,
                    "timed-out": -32001
                }
            },
            "log-level": {
                "type": "enum",
                "enumerators": {
                    "notice": 0,
                    "info": 1,
                    "warning": 2,
                    "error": 3,
                    "fatal": 4
                }
            },
            "error-instance": {
                "type": "object",
                "params": {
                    "code": {
                        "type": "error-code"
                    },
                    "message": {
                        "type": "string",
                        "optional": true
                    }
                }
            },
            "image-resolution": {
                "type": "enum",
                "enumerators": {
                    "normal": 0,
                    "high": 1
                }
            },
            "localized-resource": {
                "type": "object",
                "params": {
                    "uri": {
                        "type": "string"
                    },
                    "lang": {
                        "type": "string",
                        "optional": true
                    }
                }
            },
            "localized-image": {
                "type": "object",
                "params": {
                    "uri": {
                        "type": "string"
                    },
                    "lang": {
                        "type": "string",
                        "optional": true
                    },
                    "resolution": {
                        "type": "image-resolution",
                        "optional": true
                    }
                }
            },
            "localized-string": {
                "type": "object",
                "params": {
                    "text": {
                        "type": "string"
                    },
                    "lang": {
                        "type": "string",
                        "optional": true
                    }
                }
            },
            "client-info": {
                "type": "object",
                "params": {
                    "name": {
                        "type": "string"
                    },
                    "version": {
                        "type": "string"
                    },
                    "capabilities": {
                        "type": "json-object"
                    }
                }
            },
            "action": {
                "type": "object",
                "params": {
                    "command": {
                        "type": "string"
                    },
                    "params": {
                        "type": "json-object"
                    },
                    "target": {
                        "type": "string",
                        "optional": true
                    }
                }
            },
            "dependency": {
                "type": "object",
                "params": {
                    "name": {
                        "type": "string"
                    },
                    "version": {
                        "type": "string",
                        "optional": true
                    }
                }
            },
            "component-manifest": {
                "type": "object",
                "params": {
                    "name": {
                        "type": "string"
                    },
                    "version": {
                        "type": "string"
                    },
                    "capabilities": {
                        "type": "json-object",
                        "optional": true
                    },
                    "contributions": {
                        "type": "json-object",
                        "optional": true
                    },
                    "dependencies": {
                        "type": "array",
                        "item-type": "dependency",
                        "optional": true
                    }
                }
            },
            "launcher-info": {
                "type": "object",
                "params": {
                    "type": {
                        "type": "string"
                    },
                    "requirements": {
                        "type": "json-object",
                        "optional": true
                    },
                    "args": {
                        "type": "array",
                        "item-type": "string",
                        "optional": true
                    }
                }
            },
            "executable-info": {
                "type": "object",
                "params": {
                    "name": {
                        "type": "array",
                        "item-type": "localized-string"
                    },
                    "version": {
                        "type": "string"
                    },
                    "executable": {
                        "type": "string"
                    },
                    "launcher": {
                        "type": "launcher-info"
                    },
                    "args": {
                        "type": "array",
                        "item-type": "string",
                        "optional": true
                    },
                    "icon": {
                        "type": "array",
                        "item-type": "localized-image",
                        "optional": true
                    },
                    "description": {
                        "type": "array",
                        "item-type": "localized-string",
                        "optional": true
                    },
                    "region": {
                        "type": "string",
                        "optional": true
                    },
                    "id": {
                        "type": "string",
                        "optional": true
                    }
                }
            },
            "extension-info": {
                "type": "object",
                "params": {
                    "name": {
                        "type": "array",
                        "item-type": "localized-string"
                    },
                    "version": {
                        "type": "string"
                    },
                    "executable": {
                        "type": "string"
                    },
                    "launcher": {
                        "type": "launcher-info"
                    },
                    "args": {
                        "type": "array",
                        "item-type": "string",
                        "optional": true
                    },
                    "icon": {
                        "type": "array",
                        "item-type": "localized-image",
                        "optional": true
                    },
                    "description": {
                        "type": "array",
                        "item-type": "localized-string",
                        "optional": true
                    },
                    "id": {
                        "type": "string",
                        "optional": true
                    },
                    "capabilities": {
                        "type": "json-object",
                        "optional": true
                    },
                    "contributions": {
                        "type": "json-object",
                        "optional": true
                    },
                    "dependencies": {
                        "type": "array",
                        "item-type": "dependency",
                        "optional": true
                    }
                }
            }
        },
        "methods": {
            "initialize": {
                "virtual": true,
                "params": {
                    "client": {
                        "type": "client-info"
                    }
                },
                "returns": {
                    "extension": {
                        "type": "extension-info"
                    }
                }
            },
            "activate": {
                "virtual": true,
                "params": {
                    "settings": {
                        "type": "json-object"
                    }
                }
            },
            "deactivate": {
                "virtual": true
            },
            "shutdown": {
                "handler": "shutdown"
            },
            "component/activate": {
                "handler": "activateComponent",
                "params": {
                    "id": {
                        "type": "string"
                    }
                }
            },
            "component/deactivate": {
                "handler": "deactivateComponent",
                "params": {
                    "id": {
                        "type": "string"
                    }
                }
            },
            "extension/load": {
                "handler": "loadExtension",
                "params": {
                    "id": {
                        "type": "string"
                    }
                }
            },
            "extension/unload": {
                "handler": "unloadExtension",
                "params": {
                    "id": {
                        "type": "string"
                    }
                }
            },
            "extension/install": {
                "handler": "installExtension",
                "params": {
                    "path": {
                        "type": "string"
                    }
                },
                "returns": {
                    "id": {
                        "type": "string"
                    }
                }
            },
            "extension/remove": {
                "handler": "removeExtension",
                "params": {
                    "id": {
                        "type": "string"
                    }
                }
            },
            "settings/set": {
                "handler": "handleSettingsSet",
                "params": {
                    "path": {
                        "type": "string"
                    },
                    "value": {
                        "type": "json"
                    }
                }
            },
            "settings/get": {
                "handler": "handleSettingsGet",
                "params": {
                    "path": {
                        "type": "string"
                    }
                },
                "returns": {
                    "value": {
                        "type": "json"
                    },
                    "schema": {
                        "type": "json-object"
                    }
                }
            },
            "object/create": {
                "handler": "handleObjectCreate",
                "params": {
                    "name": {
                        "type": "string"
                    },
                    "interface": {
                        "type": "string"
                    }
                },
                "returns": {
                    "object": {
                        "type": "number"
                    }
                }
            },
            "object/destroy": {
                "handler": "handleObjectDestroy",
                "params": {
                    "object": {
                        "type": "number"
                    }
                }
            },
            "object/get-name": {
                "handler": "handleObjectGetName",
                "params": {
                    "object": {
                        "type": "number"
                    }
                },
                "returns": {
                    "name": {
                        "type": "string"
                    }
                }
            },
            "object/find": {
                "handler": "handleFindObject",
                "params": {
                    "interface-name": {
                        "type": "string"
                    },
                    "object-name": {
                        "type": "string"
                    }
                },
                "returns": {
                    "object": {
                        "type": "number"
                    }
                }
            },
            "object/get-list": {
                "handler": "handleObjectGetList",
                "params": {
                    "interface": {
                        "type": "string"
                    }
                },
                "returns": {
                    "objects": {
                        "type": "array",
                        "item-type": "number"
                    }
                }
            },
            "object/call": {
                "handler": "handleObjectCall",
                "params": {
                    "object": {
                        "type": "number"
                    },
                    "method": {
                        "type": "string"
                    },
                    "params": {
                        "type": "json"
                    }
                },
                "returns": {
                    "result": {
                        "type": "json"
                    }
                }
            }
        },
        "notifications": {
            "cancel": {
                "virtual": true,
                "params": {
                    "id": {
                        "type": "number"
                    }
                }
            },
            "object/notify": {
                "handler": "handleObjectNotify",
                "params": {
                    "object": {
                        "type": "number"
                    },
                    "notification": {
                        "type": "string"
                    },
                    "params": {
                        "type": "json"
                    }
                }
            }
        },
        "events": {
            "settings/update": {
                "path": {
                    "type": "string"
                },
                "value": {
                    "type": "json"
                }
            },
            "object/created": {
                "interface": {
                    "type": "string"
                },
                "object": {
                    "type": "number"
                }
            }
        },
        "interfaces": {
            "external-component": {
                "methods": {
                    "call": {
                        "params": "json",
                        "returns": "json"
                    }
                },
                "notifications": {
                    "notify": {
                        "params": "json"
                    }
                }
            }
        }
    }
}